{
    "name": "alecavallo/jobs-glassdoor",
    "type": "library",
    "description": "Making it simple to integrate your application with Glassdoor Jobs API.",
    "keywords": [
        "jobs",
        "api client",
        "object",
        "glassdoor"
    ],
    "homepage": "https://github.com/alecavallo/jobs-glassdoor",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alejandro Cavallo",
            "email": "alejandro.cavallo@gmail.com",
            "homepage": "https://github.com/alecavallo"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "jobbrander/jobs-common": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.6",
        "phpunit/php-code-coverage": "~2.0",
        "mockery/mockery": ">=0.9.4",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "JobBrander\\Jobs\\Client\\Providers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobBrander\\Jobs\\Client\\Providers\\Test\\": "tests/src/"
        }
    }
}
